{
    "builds": [
        {
            "src": "fastapi_app.py",
            "use": "@vercel/python",
            "config": {
                "includeFiles": [
                    "chat_interface.html"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "fastapi_app.py"
        }
    ]
}